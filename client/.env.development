# Development environment variables for Campus Vibe Client
# Local development API URL
REACT_APP_API_URL=http://localhost:3001
REACT_APP_ENVIRONMENT=development

# Build optimizations (disabled for development)
GENERATE_SOURCEMAP=true
INLINE_RUNTIME_CHUNK=true

# Performance settings
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_CACHE_TIMEOUT=5000

# AWS specific settings (not used in development)
REACT_APP_AWS_REGION=us-east-1
REACT_APP_CDN_URL=http://localhost:3000