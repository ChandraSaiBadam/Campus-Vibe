# Staging environment variables for Campus Vibe Client
# API Gateway URL for staging environment
REACT_APP_API_URL=https://staging-api.campusvibe.com/staging
REACT_APP_ENVIRONMENT=staging

# Build optimizations
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=false
IMAGE_INLINE_SIZE_LIMIT=0

# Performance settings
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_CACHE_TIMEOUT=60000

# AWS specific settings
REACT_APP_AWS_REGION=us-east-1
REACT_APP_CDN_URL=https://staging-cdn.campusvibe.com