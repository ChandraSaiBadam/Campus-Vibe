# Production environment variables for Campus Vibe Client
# API Gateway URL - replace with actual API Gateway endpoint after deployment
# Format: https://{api-id}.execute-api.{region}.amazonaws.com/{stage}
REACT_APP_API_URL=https://zfvsyqr372.execute-api.us-east-1.amazonaws.com/prod
REACT_APP_ENVIRONMENT=production

# Build optimizations for AWS deployment
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=false
IMAGE_INLINE_SIZE_LIMIT=0

# Performance settings
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_CACHE_TIMEOUT=300000

# AWS specific settings
REACT_APP_AWS_REGION=us-east-1
REACT_APP_CDN_URL=https://your-cloudfront-distribution.cloudfront.net

# API Gateway configuration
REACT_APP_API_STAGE=prod
REACT_APP_API_KEY=

# Performance monitoring
REACT_APP_PERFORMANCE_MONITORING=true
REACT_APP_ERROR_REPORTING=true